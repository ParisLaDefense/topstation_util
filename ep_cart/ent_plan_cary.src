rout,CreComp(§r)
/* Crée un plan portant le radical §r et crée le cartouche correspondant (format A4 en position BD) */
{
  var,§dom
  
  Cas=Cre
  run,MajListe
  Ma=[ml(§r)]
  Cart=[ml(`jtCart`)]

  
  /* on détermine les dimensions du plan */
  /* aujourd'hui par défaut A0 au 1/500  */
  Lplan={LA0}
  Hplan={HA0}
  Md=[MdPlan]
  Ec=500
  
  run,CalculeMarge(Md,MaG,MaD,MaH,MaB)
  Cy=[[[Hplan-[MaH]-[MaB]]*Ec]/1000]
  Cx=[[[Lplan-[MaG]-[MaD]]*Ec]/1000]
  
  /* on créé le plan */
  [Ma]:X=[xmvue],Y=[ymvue],G=100,Cx=[Cx],Cy=[Cy],MD=[Md]
  [Ma]&V0:Nom="[Ma]",Ec=[Ec]
  [Ma]&T0:commune=0
  
  /* on créé le cartouche */
  [Cart]:Cx=110.000,Cy=153.500,MD=[MdCart],J=0
  [Cart]&V0:Nom="[Ma]:Cartouche principal",J=7,Dx=2.50,Dy=-2.50,Cx=22.00,Cy=30.70
  [Cart]&R0:P=[Ma] 
    
  /* on définie le domaine du plan */
  §dom=[upcn(§r)][[Ma]:i]
  [Ma]&D0:[§dom]
  [Cart]&D0:[§dom]
  
  /* on ajoute les instructions qui seront exécutées à l'affichage du plan */
  [Ma]&K0:gom,!tout
  [Ma]&K1:%DOM:DOM="[§dom]"
  [Ma]&K2:tra,!dom([§dom])
  
  /* on ajoute tous les composants tracés dans le domaine du plan */
  domi,[§dom],!visb*~(jtCart+jtPlan)
  
  marq,11
  
  /* on ouvre la fenêtre de saisie */
  run,InitRpqt
  run,LecArgs
  run,CreeFenetre
 
}

