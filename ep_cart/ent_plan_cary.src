rout,BfBeok
{
  run,Enreg
  !Jrpqt=0
  if,"Cas==`Mod`"
  {
    fdia
    lanc,Modifier,[jtCart]\Modifier
    qmod,ED_COMP,tx="[Ma]"
  }
  else
  {
    run,DupComp([Ma])
  }
}

rout,BfFerm
{
  if,"[modif#[Cas==`Cre`]]"
  {
    note
    qpop,"¹Enregistrer les données modifiées ?¹",j=5
    {
      qap,"¹Oui¹",dfo=1
      {
	    run,Enreg
      }
      qap,"¹Non¹"
	  {
	    !cmpe,[MaR]
	  }
    }
  }
  !Jrpqt=0
}

