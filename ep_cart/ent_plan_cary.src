rout,Valid
{
  anul,11,saut=0
  list,[base]\Listing,raz=1
  impr,!cmpe,[Ma],V0-V999
  impr,[Ma]:X=[X],Y=[Y],Z=[Z],G=[G],Cx=[Cx],Cy=[Cy],MD=[Md]
  impr,[Ma]&V0:Nom="[Nom]",Ec=[Ec]
  impr,ral,[Ma]&T8:x=[X],y=[Y]
  !Jrpqt>[Ma]
  run,RpqtPlan
  run,Repercut
  list
  exec,[base]\Listing
  qset,mod=1
  run,AfficheRbase
