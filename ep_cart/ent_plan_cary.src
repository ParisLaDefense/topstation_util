}

rout,InitRpqt
/* Déclare et instancie toutes les procédures de valeurs qui seront récupérées en cas de répercussion */
{
  /* --- Paramètres de format et position du plan ---------------------------- */
  !Jrpqt=0
	
  MdQ=0
  EcQ=0
  GQ=0
  XQ=0
  YQ=0
  CxCyQ=0
  CxQ=0
  CyQ=0
  
  ZQ=0
  PrQ=0
  CoQ=0
  LyQ=0
}

rout,LecArgs
/* Affecte aux procédures les valeurs du formulaire de positionnement du plan */
{
  X=[[Ma]:X]
  Y=[[Ma]:Y]
  Z=[[Ma]:Z]
  G=[[Ma]:G]
  Cx=[[Ma]:Cx]
  Cy=[[Ma]:Cy]
  Md=[[Ma]:MD]
  if,"[Z==``]"
  {
    Z=0
  }
  Nom=[[Ma]&V:Nom]
  if,"[Nom==``]"
  {
    Nom="[Ma]"
  }
  
  Ec=[[Ma]&V:Ec]
  if,"[Ec==``]"
  {
    Ec=250
  }
  
  run,DetFormat(CxCy,Cx,Cy,Ec,Md,Form)
  run,DetFormIV
  if,"FormIV=2"
  {
	if,"Cy>Cx"
    then
    {
      Lplan=[HA[Form]]
      Hplan=[LA[Form]]
    }
	else
	{
	  Lplan=[LA[Form]]
      Hplan=[HA[Form]]
	}
  }
  else
  {
    Hplan=[[Cy/Ec]*1000]
	Lplan=[[Cx/Ec]*1000]
  }
  
  MaR=[ml(`epBase`)]
  run,AfficheRbase

}

