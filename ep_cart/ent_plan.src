rout,LecArgs
/* Affecte aux procédures les valeurs du formulaire de positionnement du plan */
{
  X=[[Ma]:X]
  Y=[[Ma]:Y]
  Z=[[Ma]:Z]
  G=[[Ma]:G]
  Cx=[[Ma]:Cx]
  Cy=[[Ma]:Cy]
  Md=[[Ma]:MD]
  if,"[Z==``]"
  {
    Z=0
  }
  Nom=[[Ma]&V:Nom]
  if,"[Nom==``]"
  {
    Nom="[Ma]"
  }
  
  Ec=[[Ma]&V:Ec]
  if,"[Ec==``]"
  {
    Ec=250
  }
  
  Nordre=[[Ma]&T:nordre]
  Ind=[[Ma]&T:indice]
  Com=[[Ma]&T:commune]
  Proj=[[Ma]&T:projet]
  Operation=[[Ma]&T:operation]
  Titre=[[Ma]&T:titre]
  Stitre=[[Ma]&T:soustitre]
  Txt1=[[Ma]&T:texte1]
  Txt2=[[Ma]&T:texte2]
  Txt3=[[Ma]&T:texte3]
  Loca=[[Ma]&T:point]
  Doss=[[Ma]&T:dossier]
  NoPc=[[Ma]&T:nopiece]
  Dateplan=[[Ma]&T:date]
  Zon=[[Ma]&T:zone]
  Prov=[[Ma]&T:prov]
  Etude=[[Ma]&T:etude]
  Alti=[[Ma]&T:alti]
  
  run,DetFormat(CxCy,Cx,Cy,Ec,Md,Form)
  run,DetFormIV
  if,"FormIV=2"
  {
	if,"Cy>Cx"
    then
    {
      Lplan=[HA[Form]]
      Hplan=[LA[Form]]
    }
	else
	{
	  Lplan=[LA[Form]]
      Hplan=[HA[Form]]
	}
  }
  else
  {
    Hplan=[[Cy/Ec]*1000]
	Lplan=[[Cx/Ec]*1000]
  }
  
  MaR=[ml(`epBase`)]
  run,AfficheRbase

}

