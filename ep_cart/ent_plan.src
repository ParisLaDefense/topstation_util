rout,Enreg
{
  !cmpe,[MaR]
  !cmpe,epBase
  
  if,"[et(OC_CART)]=0"
  {
    !cmpe,[Cart]
  }
  
  if,"[Cas==`Mod`]"
  {
    marq,10,`¹Modification du plan [[Ma]&V0:Nom] ([Ma])¹`
  }
    if,"[Cas==`Cre`]"
  {
    marq,10,`¹Création du plan [[Ma]&V0:Nom] ([Ma])¹`
  }
  !Jrpqt>[Ma]
  if,"nmbr(`!Jrpqt`)"
  {
    list,[base]\Listing,raz=1
    run,Repercut
    list
    exec,[base]\Listing
    marq,10,`¹Répercussion des attributs¹`
  }
}

