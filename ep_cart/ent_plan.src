rout,ValidCxCy
{
  Form=[et(OP_FORM)-1]
  
  if,"[[Form]<5]"
  then
  {
    Hplan=[HA[Form]]
    Lplan=[LA[Form]]
    FormIV=2
  }
  else
  {
    Hplan=[tx(ED_H)]
    Lplan=[tx(ED_L)]
    FormIV=0
  }
  
  Cy=[[Hplan*Ec]/1000]
  Cx=[[Lplan*Ec]/1000]
  
  if,"[[et(OP_ORIENT)]-1]"
  then /* Paysage */
  {
    if,"Cy>Cx"
        {
          Cx=[Cx+Cy]
          Cy=[Cx-Cy]
          Cx=[Cx-Cy]
          Lplan=[Lplan+Hplan]
          Hplan=[Lplan-Hplan]
          Lplan=[Lplan-Hplan]
        }
  }
  else /* Portrait */
  {
    if,"Cx>Cy"
        {
          Cx=[Cx+Cy]
          Cy=[Cx-Cy]
          Cx=[Cx-Cy]
          Lplan=[Lplan+Hplan]
          Hplan=[Lplan-Hplan]
          Lplan=[Lplan-Hplan]
        }
  }
  
  run,CalculeMarge(Md,MaG,MaD,MaH,MaB)
  Cy=[Cy-[[MaH+MaB]*[Ec]/1000]]
  Cx=[Cx-[[MaG+MaD]*[Ec]/1000]]
  
  if,"Cy<0"
  then
  {
    Cy=0
  }
  if,"Cx<0"
  then
  {
    Cx=0
  }
  
  qmod,ED_H,tx=[Hplan],IV=[FormIV]
  qmod,ED_L,tx=[Lplan],IV=[FormIV]
  qmod,TX_H,tx=[Hplan],IV=[2-FormIV]
  qmod,TX_L,tx=[Lplan],IV=[2-FormIV]
  
  Rep=Plan
  run,Valid
}

