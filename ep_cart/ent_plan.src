rout,AfficheRbase
{
  /* Cette routine permet de visualiser le rectangle de base définie par le */
  /*   composant enregistré dans la pocédure MaR, en affichant le symbole   */
  /*   déterminé par la procédure BlkFen                                    */
  
  [MaR]:X=[X],Y=[Y],G=[G],PH=7,MD=JT_ENT_BL
  ral,[MaR]&v0:Pr=7,Co=6,Lw=-1
  ral,[MaR]&v1:Ly="0"
  ral,[MaR]&v5:Ex=[Cx],Ey=[Cy],Es=1,Sl=1
  ral,[MaR]&v6:Bl="[BlkFen]"
  tra,[MaR]
}

rout,BfAide
{
  !prce,JtAide_@
  JtAide_a"¹Créer/Reproduire/Modifier un point de support foncier¹"=[juCartEP]\aide\ent_plan.txt
  JtAide_b"¹Répercuter sur d'autres composants¹"=[jtCart]\aide\Repercuter.txt
  JtAide_c"¹Définir/Modifier les attributs du plan¹"=[juCartEP]\aide\ent_plan_att.txt
  aide,JtAide_@
}

rout,BfBeok
{
  run,Enreg
  !Jrpqt=0
  if,"Cas==`Mod`"
  {
    fdia
    lanc,Modifier,[jtCart]\Modifier
    qmod,ED_COMP,tx="[Ma]"
  }
  else
  {
    if,"Cas==`Rep`"
    {
      run,DupComp([Ma])
    }
    else
    {
      fdia
    }
  }
}

rout,BfFerm
{
  if,"[modif#[Cas==`Cre`]]"
  {
    note
    qpop,"¹Enregistrer les données modifiées ?¹",j=5
    {
      qap,"¹Oui¹",dfo=1
      {
	    if,"Rep==`Attr`"
		{
		  run,BfBeokAtt
		}
        run,Enreg
      }
      qap,"¹Non¹"
	  {
	    !cmpe,[MaR]
	  }
    }
  }
  !Jrpqt=0
}

rout,BfRpqt
{
  Rep=Plan
  var,§t
  §t=¹Choix des plans sur lesquels répercuter ?¹
  chsa,nom=!Jrpqt,selc=jtPlan*~[Ma],titr={§t},tri={mm}&V0:Nom,eval={mm}&V0:Nom,ok=Valid
}

rout,CreComp(§r)
/* Crée un plan portant le radical §r et crée le cartouche correspondant (format A4 en position BD) */
{
  var,§dom
  
  Cas=Cre
  run,MajListe
  Ma=[ml(§r)]

  /* on détermine les dimensions du plan */
  /* aujourd'hui par défaut A0 au 1/500  */
  Lplan={LA0}
  Hplan={HA0}
  Md=[MdPlan]
  Ec=500
  
  run,CalculeMarge(Md,MaG,MaD,MaH,MaB)
  Cy=[[[Hplan-[MaH]-[MaB]]*Ec]/1000]
  Cx=[[[Lplan-[MaG]-[MaD]]*Ec]/1000]
  
  /* on créé le plan */
  [Ma]:X=[xmvue],Y=[ymvue],G=100,Cx=[Cx],Cy=[Cy],MD=[Md]
  [Ma]&V0:Nom="[Ma]",Ec=[Ec]
  [Ma]&T0:commune=0
    
  /* on définie le domaine du plan */
  §dom=[upcn(§r)][[Ma]:i]
  [Ma]&D0:[§dom]
  
  /* on créé le cartouche */
  Cart=[ml(`jtCart`)]
  
  [Cart]:Cx=110.000,Cy=153.500,MD=[MdCart],J=0
  [Cart]&V0:Nom="[Ma]:Cartouche principal",J=7,Dx=2.50,Dy=-2.50,Cx=22.00,Cy=30.70
  [Cart]&R0:P=[Ma] 
  [Cart]&D0:[§dom]
  
  /* on ajoute les instructions qui seront exécutées à l'affichage du plan */
  [Ma]&K0:gom,!tout
  [Ma]&K1:%DOM:DOM="[§dom]"
  [Ma]&K2:tra,!dom([§dom])
  
  /* on ajoute tous les composants tracés dans le domaine du plan */
  domi,[§dom],!visb*~(jtCart+jtPlan)
  
  marq,11
  
  /* on ouvre la fenêtre de saisie */
  run,InitRpqt
  run,LecArgs
  run,CreeFenetre
}

