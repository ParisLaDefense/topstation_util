rout,DefAttr
/* Définition des variables qui porteront les valeurs des attributs */
{
  Dtcc=[[Ma]&Q0:dtc]
  Dtci=[[Ma]&Q0:id]
  
  /* Récupération de l'index de nature de l'information (0=calculé, 1=digitalisé, etc...) */
  var,§nati
  §nati=[[Ma]&Q0:in]
  if,"lwc(§nati)==`c%`"
  {
        Nati=0
  }
  else
  {
        if,"lwc(§nati)==`d%`"
        {
          Nati=1
        }
        else
        {
          if,"lwc(§nati)==`i%`"
          {
                Nati=2
          }
          else
          {
                Nati=3
          }
        }
  }
  Eddv=[[Ma]&R0:E]
  
  /* Récupération de l'index de commune et désignation des communes par un string de la forme xxyy... avec :    */
  /*                            - xx = 2 derniers caractères de l'identifiant INSEE de la commune x             */
  /*                            - yy = 2 derniers caractères de l'identifiant INSEE de la commune y, etc...     */
  var,§com
  §com=[[Ma]&T0:commune]
  Comm=
  if,"§com><``"
  {
    if,"§com=1"
    {
          Comm=26
    }
    if,"§com=2"
    {
          Comm=35
    }
    if,"§com=3"
    {
          Comm=2635
    }
    if,"§com=4"
    {
          Comm=50
    }
    if,"§com=5"
    {
          Comm=2650
    }  
    if,"§com=6"
    {
          Comm=3550
    }
    if,"§com=7"
    {
          Comm=263550
    }
    if,"§com=8"
    {
          Comm=62
    }
    if,"§com==`9`"
    {
          Comm=2662
    }
    if,"§com=10"
    {
          Comm=3562
    }
    if,"§com=11"
    {
          Comm=263562
    }
    if,"§com=12"
    {
          Comm=5062
    }
    if,"§com=13"
    {
          Comm=265062
    }
    if,"§com=14"
    {
          Comm=355062
    }
    if,"§com=15"
    {
          Comm=26355062
    }
  }
  /*------------------------------------------------------ */
  
  /* Récupération de l'index de zone (0=quartier d'affaires, etc...) */
  var,§zone
  §zone=[[Ma]&T0:zone]
  Zone=[§zone]
  
  Txt1=[[Ma]&T1:texte1]
  Titr=[[Ma]&T2:titre]
  Stit=[[Ma]&T3:soustitre]
  Txt2=[[Ma]&T4:texte2]
  Txt3=[[Ma]&T9:texte3]
  Doss=[[Ma]&T10:dossier]
  NoPc=[[Ma]&T10:nopiece]
  Dtcp=[[Ma]&T5:date]
  Indp=[[Ma]&T6:indice]
  Numo=[[Ma]&T6:nordre]
  Numg=[[Ma]&t6:ref]
  Alti=[[Ma]&T7:alti]
  Etud=[[Ma]&T7:etude]
  Prov=[[Ma]&T7:prov]
  Loca=[[Ma]&T8:point]
}

rout,DetFormat(§mod,§prc_L,§prc_H,§prc_ec,§prc_md,§prc_format)
/* Cette routine permet de d'instancier la procédure Form avec le numéro du   */
/*   format ISO A le plus proche des dimensions données par les procédures    */
/*   Lplan & Hplan ou Cx & Cy avec une tolérance définie                      */
/* Dans le cas ou aucun format ne correspond, la procédure Form est fixée à 6 */
/*   et la procédure FormIV à 0. Dans le cas contraire, Form IV ets fixée à 2 */
/* La variable §mod peut prendre deux valeurs :                               */
/*   1) LH pour utiliser les procédures Lplan & Hplan (dimensions papier du   */
/*     plan)                                                                  */
/*   2) CxCy pour utiliser les procédures Cx & Cy (dimensions "terrain" du    */
/*     plan)                                                                  */
/* L'échelle du plan est donnée par la procédure Ec                           */
{
  var,§delta
  §delta=5 /* tolérance de 5mm */
  
  var,§i,§portrait,§format
  
  var,§mag,§mad,§mah,§mab
  var,§hplan,§lplan
  
  run,CalculeMarge([§prc_md],§mag,§mad,§mah,§mab)
  
  if,"§mod><`LH`"
  then
  {
    §hplan=[[[§prc_H]/[§prc_ec]]*1000+[§mah]+[§mab]]
    §lplan=[[[§prc_L]/[§prc_ec]]*1000+[§mag]+[§mad]]
  }
  else
  {
    §hplan=[[§prc_H]]
    §lplan=[[§prc_L]]
  }
  
  if,"§hplan>§lplan"
  then
  {
    §portrait=1
  }
  else
  {
    §portrait=0
  }
  if,"[§portrait]"
  {
    §lplan=[§lplan+§hplan]
    §hplan=[§lplan-§hplan]
    §lplan=[§lplan-§hplan]
  }

  §format=6
  /* test format ISO 216 */
  for,"0->§i","§i<5","§i+1->§i"
  {
    if,"[[[§lplan]<[LA[§i]+§delta]]@[[§lplan]>[LA[§i]-§delta]]]"
    {
      if,"[[[§hplan]<[HA[§i]+§delta]]@[[§hplan]>[HA[§i]-§delta]]]"
          {
            §format=[§i]
            §lplan=[LA[§i]]
            §hplan=[HA[§i]]
          }
    }
  }
  if,"[§portrait]"
  {
    §lplan=[§lplan+§hplan]
    §hplan=[§lplan-§hplan]
    §lplan=[§lplan-§hplan]
  }
  
  [§prc_format]=[§format]
  if,"§mod==`LH`"
  then
  {
    [§prc_L]=[§lplan]
    [§prc_H]=[§hplan]
  }
}

rout,MajCom
/* Cochage des cases de communes à partir de la valeur de la procédure Comm */
{
  var,§i,§com,§a
  §i=0
  §a={Comm}
  §com=[xtr(`[§a]`,`[§i]`,2)]
  whil,"§com><``"
  {
        qmod,id=OC_C0[§com],et=1
        §i=[[§i+2]]
        §com=[xtr(`[§a]`,`[§i]`,2)]
  }
}

